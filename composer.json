{
    "name": "delishows",
    "description": "www.delishows.to",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "laravel/framework": "4.2.*",
        "cartalyst/sentry": "2.1.*",
        "soapbox/laravel-formatter": "dev-master",
        "greggilbert/recaptcha": "dev-master",
        "jeremykendall/php-domain-parser": "0.0.*",
        "baum/baum": "~1.0",
        "jeremykendall/php-domain-parser": "~1.3",
        "lusitanian/oauth": "dev-master#b7e96d0c36f17aa8a217b6be897363bb2cc93286",
        "intervention/image": "dev-master"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.cartalyst.com"
        }
    ],
    "require-dev": {
        "way/generators": "2.*",
        "barryvdh/laravel-debugbar": "1.*",
        "barryvdh/laravel-ide-helper": "1.*"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php",
            "app/library"
        ],
        "psr-0": {
            "Authority": "app/",
            "TvDb": "app/",
            "Cartalyst\\SentrySocial": "app/SentrySocial/src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
